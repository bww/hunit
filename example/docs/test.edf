    compare: semantic
    format: application/json
options:
  anchor-style: github
  format-entities: true
  table-of-contents: true
  rewrite-headers:
    Host: api.example.com
  suppress-headers: []
  allow-headers:
  - Host
  - Content-Type
  - Content-Length
